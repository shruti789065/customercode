                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="menarinimaster/components/text"
                                    text="&lt;p>Stemline founded in New York City with the mission of identifying and developing novel therapeutics for difficult-to-treat cancers&lt;/p>&#xd;&#xa;"
                                    textIsRich="true"/>
