<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[mobile/groups/responsive]"
        cq:isDelivered="{Boolean}true"
        cq:template="/conf/menarini-codifi/settings/wcm/templates/menarini---content-page"
        jcr:isCheckedOut="{Boolean}true"
        jcr:mixinTypes="[mix:versionable]"
        jcr:primaryType="cq:PageContent"
        jcr:uuid="b8fbb98c-0be4-46b2-ba6d-0272a2491ce1"
        sling:resourceType="menarinimaster/components/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="menarinimaster/components/container"
            layout="responsiveGrid">
            <experiencefragment-header
                cq:styleIds="[1653640494827]"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="menarinimaster/components/experiencefragment"
                fragmentVariationPath="/content/experience-fragments/menarini-codifi/en/site/header/master"/>
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="menarinimaster/components/container"
                editable="{Boolean}true"
                layout="responsiveGrid"/>
            <experiencefragment-footer
                cq:styleIds="[1653640499553]"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="menarinimaster/components/experiencefragment"
                fragmentVariationPath="/content/experience-fragments/menarini-codifi/en/site/footer/master"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}768"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
