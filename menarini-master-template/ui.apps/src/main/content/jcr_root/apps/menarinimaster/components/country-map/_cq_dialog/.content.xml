<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Country Map"
          sling:resourceType="cq/gui/components/authoring/dialog">
	<content
			jcr:primaryType="nt:unstructured"
			sling:resourceType="granite/ui/components/foundation/container"
			class="cq-dialog-content-page">
		<items jcr:primaryType="nt:unstructured">
			<columns
					jcr:primaryType="nt:unstructured"
					sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
					margin="{Boolean}true">
				<items jcr:primaryType="nt:unstructured">
					<coloums
							jcr:primaryType="nt:unstructured"
							jcr:title="section"
							sling:resourceType="granite/ui/components/foundation/section"
							margin="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
						<heading jcr:primaryType="nt:unstructured"
						         sling:resourceType="granite/ui/components/foundation/heading"
						         class="coral-Heading coral-Heading--4" level="{Long}4"
						         text="Map Pins"/>
						<well jcr:primaryType="nt:unstructured"
						      sling:resourceType="granite/ui/components/foundation/container">
							<layout jcr:primaryType="nt:unstructured"
							        sling:resourceType="granite/ui/components/foundation/layouts/well"/>
							<items jcr:primaryType="nt:unstructured">
								<location
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Location Label"
										name="./locationLabel"/>
								<manufacturing
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Manufacturing Label"
										name="./manufacturingLabel"/>
								<local-offices
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Local Offices Label"
										name="./localOfficesLabel"/>
								<logistics
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Logistics Label"
										name="./logisticsLabel"/>
							</items>
						</well>
							<googleKey
									jcr:primaryType="nt:unstructured"
									sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
									fieldLabel="Google Maps Key"
									name="./googleKey"/>
						</items>
					</coloums>
				</items>
			</columns>
		</items>
	</content>
</jcr:root>