                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="menarinimaster/components/text"
                            text="&lt;div class=&quot;tableScroll&quot;>&lt;table cellpadding=&quot;1&quot; cellspacing=&quot;0&quot; border=&quot;1&quot; width=&quot;100%&quot;>&#xd;&#xa;&lt;tbody>&lt;tr>&lt;td width=&quot;40%&quot; style=&quot;text-align: center; background-color: rgb(22,102,113); color: rgb(255,255,255);&quot;>&lt;b>BERLIN-ADLERSHOF&lt;/b>&lt;/td>&#xd;&#xa;&lt;td width=&quot;30%&quot; style=&quot;text-align: center; background-color: rgb(22,102,113); color: rgb(255,255,255);&quot;>&lt;b>BERLIN-BRITZ&lt;/b>&lt;/td>&#xd;&#xa;&lt;td width=&quot;33%&quot; style=&quot;text-align: center; background-color: rgb(22,102,113); color: rgb(255,255,255);&quot;>&lt;b>KALUGA&lt;/b>&lt;/td>&#xd;&#xa;&lt;/tr>&lt;tr>&lt;td>&lt;ul>&#xd;&#xa;&lt;li>Suppositorien&lt;/li>&#xd;&#xa;&lt;li>Liquida&lt;/li>&#xd;&#xa;&lt;li>Konfektionierung aller&amp;nbsp;Arzneimittelformen&lt;/li>&#xd;&#xa;&lt;/ul>&#xd;&#xa;&lt;/td>&#xd;&#xa;&lt;td>&lt;ul>&#xd;&#xa;&lt;li>Tabletten&lt;/li>&#xd;&#xa;&lt;li>Filmtabletten&lt;/li>&#xd;&#xa;&lt;li>Dragees&lt;/li>&#xd;&#xa;&lt;/ul>&#xd;&#xa;&lt;/td>&#xd;&#xa;&lt;td>&lt;ul>&#xd;&#xa;&lt;li>Tabletten&lt;/li>&#xd;&#xa;&lt;li>Filmtabletten&lt;/li>&#xd;&#xa;&lt;li>Dragees&lt;/li>&#xd;&#xa;&lt;/ul>&#xd;&#xa;&lt;/td>&#xd;&#xa;&lt;/tr>&lt;/tbody>&lt;/table>&#xd;&#xa;&lt;/div>&#xd;&#xa;"
                            textIsRich="true"/>
