<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[mobile/groups/responsive]"
        cq:template="/conf/fondazione/settings/wcm/templates/fondazione---signin"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="fondazione/components/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="fondazione/components/container"
            layout="responsiveGrid">
            <experiencefragment-header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="fondazione/components/experiencefragment"
                fragmentVariationPath="/content/experience-fragments/fondazione/us/en/site/header/master"/>
            <heroslim
                jcr:created="{Date}2024-10-22T14:23:07.410+02:00"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2024-10-22T14:23:07.410+02:00"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="fondazione/components/heroSlim"
                editable="{Boolean}true"/>
            <breadcrumb
                jcr:created="{Date}2024-10-22T14:23:41.719+02:00"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2024-10-22T14:23:41.719+02:00"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="fondazione/components/breadcrumb"/>
            <signinform
                jcr:created="{Date}2024-10-22T14:23:59.383+02:00"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2024-10-22T14:23:59.383+02:00"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="fondazione/components/signinform"
                editable="{Boolean}true"/>
            <spacer
                jcr:created="{Date}2024-10-23T11:00:35.816+02:00"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2024-10-23T11:00:45.768+02:00"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="fondazione/components/spacer"
                editable="{Boolean}true"/>
        </root>
        <experiencefragment-footer
            jcr:primaryType="nt:unstructured"
            sling:resourceType="fondazione/components/experiencefragment"
            fragmentVariationPath="/content/experience-fragments/fondazione/en/site/footer/master"/>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}768"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
