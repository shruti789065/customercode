<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:crx="http://www.day.com/crx/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <allow
        jcr:primaryType="rep:GrantACE"
        rep:principalName="approver-relife-th"
        rep:privileges="{Name}[jcr:versionManagement,jcr:read,jcr:modifyAccessControl,rep:write,crx:replicate,jcr:lockManagement]"/>
    <allow1
        jcr:primaryType="rep:GrantACE"
        rep:principalName="approver-relife-th"
        rep:privileges="{Name}[jcr:removeNode,jcr:nodeTypeManagement,jcr:removeChildNodes,jcr:addChildNodes]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/jcr:content*"/>
    </allow1>
    <allow2
        jcr:primaryType="rep:GrantACE"
        rep:principalName="editor-relife-th"
        rep:privileges="{Name}[jcr:versionManagement,jcr:read,jcr:modifyAccessControl,rep:write,jcr:lockManagement]"/>
    <deny3
        jcr:primaryType="rep:DenyACE"
        rep:principalName="editor-relife-th"
        rep:privileges="{Name}[crx:replicate]"/>
    <allow4
        jcr:primaryType="rep:GrantACE"
        rep:principalName="editor-relife-th"
        rep:privileges="{Name}[jcr:removeNode,jcr:nodeTypeManagement,jcr:removeChildNodes,jcr:addChildNodes]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/jcr:content*"/>
    </allow4>
</jcr:root>
